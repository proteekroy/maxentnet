
c = load('../data/toy/toy_problem_label_color.txt');
s = load('../data/toy/toy_problem_label_shape.txt');
for i=0.1:0.1:0.9
    
    filename = strcat('maxent_gaussian_', num2str(i),'.txt');
    z = load(filename);
    figure;
    hold all;
    box on;
    grid on;
    index = (c ==0) & (s==0);
    h = scatter(z(index,1), z(index,2), 100, 's' , 'MarkerFaceColor', 'g', 'MarkerEdgeColor', 'g', 'LineWidth', 1);
    %h.MarkerFaceAlpha = .5;
    

%     legend('No Privacy (HV: 0.0)','MaxEnt-ARL (HV: 0.59)', 'ML-ARL (HV: 0.42)')
%     xlabel('Target Accuracy (%)')
%     ylabel('Sensitive Accuracy (%)')
%     zlabel('Entropy of Adversary')
%     set(gca,'FontSize',18)
end